<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="set-stepzen-debug-header">
    <DisplayName>Set StepZen Debug Header</DisplayName>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Authorization">APIKEY {propertyset.StepZen.adminkey}</Header>
            <Header name="StepZen-Debug-Level">1</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
